{
  "name": "CUSTOM_NAME",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "graphql:codegen": "dotenv -- graphql-codegen --config ./.graphqlrc.yml",
    "graphql:codegen:watch": "dotenv -- graphql-codegen --watch --config ./.graphqlrc.yml",
    "dev": "run-p graphql:codegen:watch start",
    "start": "react-scripts start",
    "graphql:codegen:clear": "find ./src -name \"__generated__\" -exec rm -rf '{}' +",
    "build:dev": "react-scripts build --mode development",
    "build:staging": "dotenv -e .env.staging react-scripts build NODE_ENV=production",
    "build:feature": "dotenv -e .env.staging react-scripts build NODE_ENV=production",
    "build:master": "dotenv -e .env.master react-scripts build NODE_ENV=production",
    "build:release": "dotenv -e .env.release react-scripts build NODE_ENV=production",
    "test:unit:ci": "jest -u",
    "test:unit": "jest -u",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cypress:ci",
    "cypress:ci": "cypress run --headless --record",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress",
    "cypress": "cypress open --project .",
    "lint:lint": "eslint --ext .ts,.tsx src --color --fix",
    "lint:pretify": "prettier --write 'src/**/*.{ts,tsx,scss,json,graphql}'",
    "lint": "run-s lint:pretify lint:lint",
    "commit": "git-cz",
    "version:sync": "git fetch --tags",
    "packages:purge": "rm -rf node_modules",
    "packages:restore": "npm run packages:purge && npm i",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "graphql": "15.5.0",
    "graphql-language-service-interface": "2.4.0",
    "**/apollo-language-server/graphql": "^15.0.0",
    "**/@types/graphql-upload/graphql": "^15.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@babel/eslint-parser": "^7.14.5",
    "@sentry/react": "^6.6.0",
    "@sentry/tracing": "^6.6.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/history": "^4.7.9",
    "@types/ramda": "^0.27.40",
    "date-fns": "^2.22.1",
    "graphql": "^15.5.0",
    "pojo-observer": "^1.1.9",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/import-types-preset": "^1.18.2",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/near-operation-file-preset": "^1.18.1",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.5",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.1",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql-cli": "^4.0.0",
    "husky": "^6.0.0",
    "jest": "26.6.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.34.1",
    "start-server-and-test": "^1.12.5",
    "ts-jest": "^26.4.2",
    "typescript": "4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
